DELETE FROM Staff
WHERE member_id IN (SELECT member_id FROM Athlete);

BEGIN;
CREATE TEMP TABLE newoffs ON COMMIT DROP AS
  SELECT member_id FROM Staff WHERE random()<0.2; -- Choose about 20%
INSERT INTO Official (member_id)
  SELECT member_id FROM newoffs;
DELETE FROM Staff WHERE member_id IN (
  SELECT member_id FROM newoffs);
COMMIT;


